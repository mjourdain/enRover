<configuration>
  <Map size="800,600"/><!--in pixels-->
  <Scale px="1"
         meters="30"/>
  <!--
  <Mobile id=""
          location="10m,10km" || "10km,10m" || "10m,10m" || "10km,10km"
          network : prefered network ="GSM" || "UMTS"
          p : maximum emission power of the ms [dBm]
          pe : emission power [dBm]
          ge : emission gain [dB]
  />
  -->
  <Mobile id="1"
          location="10m,10m"
          network="GSM"
          pe="33"
          ge="0"
          p="36"/>
  <Mobile id="2"
          location="1km,3km"
          network="GSM"
          pe="33"
          ge="0"
          p="36"/>
  <Mobile id="3"
          network="UMTS"
          pe="33"
          ge="0"
          p="36"/>
  <Mobile id="4"
          location="2km,6km"
